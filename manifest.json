{
  "manifest_version": 3,
  "name": "Google Tasks to Calendar",
  "version": "1.0",
  "description": "Add Google Tasks to your Calendar with one click",
  "permissions": [
    "identity",
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "https://www.googleapis.com/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://tasks.google.com/*"],
      "js": ["content.js"]
    }
  ],
  "oauth2": {
    "client_id": "727538399732-jqihbqrlc9s6dk46r50hgk11g41329db.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/calendar.events"
    ]
  },
  "key": ""
}
